@model List<NoteModel>

<div>
    <h1>@ViewBag.Tag.Title</h1>
    <div class="note-container">
        @if (Model.Count() == 0)
        {
            <p>Deze Tag heeft geen notities</p>
        }
        else
        {
            @foreach (NoteModel note in Model)
            {
                @Html.Partial("_Note", note)
            }
        }
    </div>
</div>
<div>
    <a asp-controller="Home" asp-action="Index">Terug naar overzicht</a>
</div>